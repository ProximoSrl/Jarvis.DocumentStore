    <add key="vs:EnableBrowserLink" value="false" />
    
    <add key="ikvm:java.system.class.loader" value="TikaOnDotNet.MySystemClassLoader, TikaOnDotNet" />

    <add key="JARVIS_DOCUMENTSTORE_ALLOWED_FILE_TYPES" value="*" />
    
    <!--<add key="polling-interval-ms" value="1000" />
    <add key="poller-delayed-start" value="2000" />-->

